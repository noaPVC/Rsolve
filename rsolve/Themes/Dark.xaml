<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <!-- Colors and Brushes -->
    <SolidColorBrush x:Key="Primary" Color="#1B1B1B" />
    <SolidColorBrush x:Key="PrimaryText" Color="#FFFFFF" />

    <!-- TitleBar control Brushes -->
    <SolidColorBrush x:Key="ToolbarGrayHover" Color="#808080" Opacity="0.4" />
    <SolidColorBrush x:Key="ToolbarForegroundIconWhite" Color="#FFFFFF" />

    <!-- Images -->
    <BitmapImage x:Key="TitleBarLogo" UriSource="/Assets/Images/Header/rsolve_header_dark.png"/>
    <BitmapImage x:Key="SplashScreenImage" UriSource="/Assets/Images/Splash/rsolve_splash_dark.png"/>
</ResourceDictionary>